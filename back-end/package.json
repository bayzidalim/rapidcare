{
  "name": "rapidcare-backend",
  "version": "1.0.0",
  "description": "RapidCare Backend API - Emergency medical resource booking platform backend services",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "build": "echo 'No build step required for Node.js'",
    "seed": "node utils/seeder.js",
    "seed:financial": "node utils/financialSeeder.js",
    "seed:social": "node seed-social-posts.js",
    "assign:credentials": "node utils/assignHospitalCredentials.js",
    "view:credentials": "node utils/viewHospitalCredentials.js",
    "update:pricing-5x": "node update-pricing-5x.js",
    "test": "NODE_ENV=test mocha 'tests/**/*.test.js' --timeout 30000 --exit",
    "test:models": "NODE_ENV=test mocha 'tests/models/**/*.test.js' --timeout 30000 --exit",
    "test:auth": "NODE_ENV=test mocha 'tests/auth/**/*.test.js' --timeout 30000 --exit",
    "test:watch": "NODE_ENV=test mocha 'tests/**/*.test.js' --watch --timeout 30000",
    "lint": "eslint ."
  },
  "keywords": [
    "rapidcare",
    "emergency-care",
    "healthcare-api",
    "medical-booking",
    "express",
    "mongo",
    "jwt-auth"
  ],
  "author": "RapidCare Team",
  "license": "MIT",
  "type": "commonjs",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.0.1",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^9.1.6",
    "node-cron": "^4.2.1"
  },
  "devDependencies": {
    "chai": "^4.3.10",
    "eslint": "^9.36.0",
    "mocha": "^10.2.0",
    "mongodb-memory-server": "^11.0.1",
    "nodemon": "^3.1.10",
    "sinon": "^17.0.1",
    "supertest": "^6.3.3"
  }
}